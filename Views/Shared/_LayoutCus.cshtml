                    <li class="ruby-menu-right ruby-menu-social ruby-menu-search">
                        <form id="masthead-search">
                            @using (Html.BeginForm("SearchDatCho", "BookTicket", FormMethod.Post, new { enctype = "multipart/form-data" }))
                            {
                                <input type="text" name="madatcho" aria-labelledby="search-label" placeholder="Search&hellip;" class="draw">
                                <button type="submit">&rarr;</button>
                            }

                        </form>
                    </li>

                </ul>
            </div>
        </div>
